[
  {
    "name": "toebeans",
    "image": "022111582403.dkr.ecr.ap-northeast-1.amazonaws.com/toebeans:latest",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "toebeans",
        "awslogs-group": "/ecs/toebeans"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 8080,
        "hostPort": 8080
      }
    ],
    "command": [
      "/backend"
    ],
    "secrets": [
      {
        "name": "APP_ENV",
        "valueFrom": "/app_env"
      },
      {
        "name": "CSRF_AUTH_KEY",
        "valueFrom": "/csrf_auth_key"
      },
      {
        "name": "DB_PASSWORD",
        "valueFrom": "/db_password"
      },
      {
        "name": "DOMAIN",
        "valueFrom": "/domain"
      },
      {
        "name": "GITHUB_TOKEN",
        "valueFrom": "/github_token"
      },
      {
        "name": "GOOGLE_API_KEY",
        "valueFrom": "/google_api_key"
      },
      {
        "name": "GRACEFUL_SHUTDOWN_TIMEOUT_SECOND",
        "valueFrom": "/graceful_shutdown_timeout_second"
      },
      {
        "name": "JWT_SECRET_KEY",
        "valueFrom": "/jwt_secret_key"
      },
      {
        "name": "LOG_LEVEL",
        "valueFrom": "/log_level"
      }
    ]
  }
]